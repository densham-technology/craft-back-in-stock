{
    "name": "densham-technology/craft-back-in-stock",
    "description": "Plugin giving customers the ability to add their email to a list which will allow them to be notified when a product that is out of stock is restocked.",
    "type": "craft-plugin",
    "version": "0.1.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-back-in-stock"
    ],
    "support": {
        "docs": "https://github.com/densham-technology/craft-back-in-stock/blob/master/README.md",
        "issues": "https://github.com/densham-technology/craft-back-in-stock/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Densham Technology",
            "homepage": "https://densham.technology"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "craftcms/commerce": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "denshamtechnology\\backinstock\\": "src/"
        }
    },
    "extra": {
        "name": "Back in Stock",
        "handle": "back-in-stock",
        "developer": "Densham Technology",
        "developerUrl": "https://densham.technology",
        "documentationUrl": "https://github.com/densham-technology/craft-back-in-stock/blob/master/README.md",
        "changelogUrl": "https://github.com/densham-technology/craft-back-in-stock/CHANGELOG.md",
        "components": {
            "subscriptions": "denshamtechnology\\backinstock\\services\\Subscriptions"
        },
        "class": "denshamtechnology\\backinstock\\BackInStock"
    }
}
